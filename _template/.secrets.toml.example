# Client Secrets Template
# Copy this to .secrets.toml and fill in your credentials
# IMPORTANT: .secrets.toml is gitignored and should NEVER be committed

# Snowflake credentials
SNOWFLAKE_ACCOUNT = "your_account.region"
SNOWFLAKE_USER = "your_user"
SNOWFLAKE_ROLE = "your_role"

# Private key authentication (base64 encoded PEM)
PRIVATE_KEY_PEM_B64 = "base64_encoded_private_key_here"
PRIVATE_KEY_PASSPHRASE = "your_passphrase"

# OR password authentication (not recommended)
# SNOWFLAKE_PASSWORD = "your_password"

# Admin password for setup page
